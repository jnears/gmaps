<h1>Create Event</h1>
<%= form_for @event do |f| %>
  <%= f.label :title, 'title of event' %>
  <%= f.text_field :title %>
  <%= f.label :body, 'Address' %>
  <%= f.text_field :body %>
  <%= f.collection_select(:company_id, Company.all, :id,  :name, { prompt: true }) %>
  <%= link_to "Add a new venue", new_company_path %>
  <%= f.submit 'UPDATE' %>
<% end %>